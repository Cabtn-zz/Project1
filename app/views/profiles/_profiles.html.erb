<div class="panel panel-default">
  <div class="panel-heading">
    <div class="panel-title">
      <h1>Your Doctors</h1>
    </div>
  </div>
  <div class="panel-body">
    <table>
      <thead>
        <tr>
          <th>Name <br></th>
          <th>City <br></th>
          <th>State <br></th>
          <th>Degree <br></th>
          <th>Speciality <br></th>
          <th colspan="3"></th>
        </tr>
      </thead>

      <tbody>
        <% @profiles.each do |profile| %>
          <tr>
            <td><%= profile.name %></td>
            <td><%= profile.city %></td>
            <td><%= profile.state %></td>
            <td><%= profile.doctor_profile.try(:degree) %></td>
            <td><%= profile.doctor_profile.try(:speciality) %></td>
          </tr>
        <% end %>
      </tbody>
    </table>
  </div>
</div>

