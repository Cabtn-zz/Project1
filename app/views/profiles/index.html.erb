<h1>Your Doctors</h1>

<table>
  <thead>
    <tr>
      <th>Name <br></th>

      <th>City <br></th>

      <th>Speciality <br></th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @profiles.each do |profile| %>
      <tr>
        <td><%= profile.name %></td>
        <td><%= profile.city %></td>
        <td><%= profile.state %></td>
        <td><%#= link_to 'Show', profile %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

